<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core" level="3" version="1">
  <model>
    <listOfCompartments>
      <compartment id="cell" constant="false" name="cell" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="exp" constant="false" hasOnlySubstanceUnits="true" name="exp" boundaryCondition="false" initialAmount="0" compartment="cell"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="exp_degradation" name="exp_degradation" reversible="false" fast="false">
        <listOfReactants>
          <speciesReference constant="true" species="exp" stoichiometry="1"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">          
            <apply>
              <divide/>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <ci> y </ci>
                  <ci> exp </ci>
                </apply>
                <ci> K_d </ci>
              </apply>
              <apply>
                <plus/>
                <cn type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <ci> exp </ci>
                  <ci> K_d </ci>
                </apply>
              </apply>
            </apply>
          </math>
                  <listOfLocalParameters>
            <localParameter id="y" name="y" value="1"/>
            <localParameter id="K_d" name="K_d" value="1"/>
          </listOfLocalParameters>
        </kineticLaw>
      </reaction>
      <reaction id="exp_expression" name="exp_expression" reversible="false" fast="false">
        <listOfProducts>
          <speciesReference constant="true" species="exp" stoichiometry="1"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">          
            <ci> k_EXE </ci>
          </math>
                  <listOfLocalParameters>
            <localParameter id="k_EXE" name="k_EXE" value="1"/>
          </listOfLocalParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>